<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="16dp"
    android:orientation="vertical"
    >
    <TableRow  android:background="@drawable/bg_white_conner_16">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="9"
            android:padding="10dp"
            android:textColor="@color/white"
            android:text="@string/tuoi"
            android:textSize="20sp"
            />
        <ImageView
            android:id="@+id/image1"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"

            app:srcCompat="@drawable/ic_action" />


        />

    </TableRow>

    <TableRow  android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:background="@color/white">
        <EditText
            android:layout_marginTop ="5dp"
            android:id="@+id/nhap1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text=""
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"
            android:gravity="center"
            />
    </TableRow>
    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:background="@color/white">

        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/s71"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="7"
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"/>

        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/s81"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="8"
            android:padding="10dp"


            android:background="@drawable/vien"            />
        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/s91"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="9"
            android:padding="10dp"
            android:layout_marginLeft="5dp"

            android:background="@drawable/vien"
            />

        <ImageView
            android:layout_marginTop ="5dp"
            android:id="@+id/imagexoa1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"
            app:srcCompat="@drawable/ic_action_xoa" />


        />

    </TableRow>


    <TableRow  android:background="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <Button  android:layout_width="0dp"
            android:layout_marginTop ="5dp"
            android:layout_weight="1"
            android:id="@+id/s41"
            android:padding="10dp"
            android:textSize="30sp"
            android:text="4"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"/>

        <Button  android:layout_width="0dp"
            android:layout_marginTop ="5dp"
            android:layout_weight="1"
            android:id="@+id/s51"
            android:textSize="30sp"
            android:text="5"
            android:padding="10dp"
            android:background="@drawable/vien"            />
        <Button  android:layout_width="0dp"
            android:layout_marginTop ="5dp"
            android:layout_weight="1"
            android:id="@+id/s61"
            android:textSize="30sp"
            android:text="6"
            android:padding="10dp"
            android:layout_marginLeft="5dp"

            android:background="@drawable/vien"
            />
        <TextView
            android:layout_marginTop ="5dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text=""
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"

            />

    </TableRow>

    <TableRow  android:background="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/s11"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="1"
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"/>

        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/s21"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="2"
            android:padding="10dp"
            android:background="@drawable/vien"            />
        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/s31"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="3"
            android:padding="10dp"
            android:layout_marginLeft="5dp"

            android:background="@drawable/vien"
            />
        <Button
            android:layout_marginTop ="5dp"
            android:id="@+id/C1"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="C"
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"
            android:textColor="#534340"
            />

    </TableRow>

    <TableRow  android:background="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        >

        <Button
            android:layout_marginBottom="5dp"
            android:layout_marginTop ="5dp"
            android:id="@+id/s01"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="0"
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/vien"/>


        <Button
            android:layout_marginBottom="5dp"
            android:layout_marginTop ="5dp"
            android:id="@+id/s0011"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="00"
            android:padding="10dp"


            android:background="@drawable/vien"            />
        <TextView
            android:layout_marginTop ="5dp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text=""
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"

            />
        <Button
            android:layout_marginBottom="5dp"
            android:id="@+id/sbang1"
            android:layout_marginTop ="5dp"

            android:layout_width="0dp"
            android:layout_weight="1"
            android:textSize="30sp"
            android:text="="
            android:textColor="@color/white"
            android:padding="10dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:background="@drawable/btnbang"
            />

    </TableRow>




</TableLayout>